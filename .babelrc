{
  "presets": [
    [
      "env",
      {
        "loose": true,
        "debug": false,
        "targets": {
          "browsers": [">1%", "last 3 versions"]
        }
      }
    ],
    "react"
  ],
  "retainLines": true,
  "sourceMaps": "both",
  "plugins": [
    "transform-object-rest-spread",
    "transform-class-properties",
    "lodash",
    "transform-runtime"
  ]
}
